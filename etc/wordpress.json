{
  "consul": "consul:8500",
  "services": [
    {
      "name": "wordpress",
      "port": 80,
      "publicIp": true,
      "health": "/usr/bin/curl --fail -s http://localhost/health",
      "poll": 10,
      "ttl": 25
    }
  ],
  "backends": [
    {
      "name": "mysql",
      "poll": 7,
      "onChange": "/opt/containerbuddy/wp-config.sh"
    }
  ]
}